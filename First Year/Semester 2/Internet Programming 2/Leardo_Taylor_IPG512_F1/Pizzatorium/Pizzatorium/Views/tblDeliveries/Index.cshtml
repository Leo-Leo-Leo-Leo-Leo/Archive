@model IEnumerable<Pizzatorium.Models.tblDelivery>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.dPhoto)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tblPerson.dName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tblSuburb.dSuburb)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.dPhoto)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tblPerson.dName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tblSuburb.dSuburb)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.dDeliveryID }) |
            @Html.ActionLink("Details", "Details", new { id=item.dDeliveryID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.dDeliveryID })
        </td>
    </tr>
}

</table>
